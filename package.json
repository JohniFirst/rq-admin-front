{
	"name": "RQ Admin",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev-mock": "vite --mode mock",
		"build": "tsc -b && pnpm run biome && vite build",
		"build-gzip": "vite build --mode gzip",
