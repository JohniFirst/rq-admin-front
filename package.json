{
  "name": "RQ Admin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://JohniFirst.github.io/rq-admin-front",
  "scripts": {
    "dev": "vite",
    "dev-mock": "vite --mode mock",
    "build": "tsc -b && vite build",
    "build-gzip": "vite build --mode gzip",
    "build-analyze": "vite build --mode analyzer",
    "preview": "vite preview",
    "update": "pnpm update",
    "reinstall": "npx rimraf node_modules && pnpm install",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "generate:menu": "node scripts/generate-menu.mjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "types": "tsc -b",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@bytemd/plugin-frontmatter": "^1.22.0",
    "@bytemd/plugin-gfm": "^1.22.0",
    "@bytemd/plugin-highlight": "^1.22.0",
    "@bytemd/plugin-math": "^1.22.0",
    "@bytemd/plugin-medium-zoom": "^1.22.0",
    "@bytemd/plugin-mermaid": "^1.22.0",
    "@bytemd/react": "^1.22.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/rrule": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.8.2",
    "antd": "^5.26.6",
    "axios": "^1.11.0",
    "bytemd": "^1.22.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "es-toolkit": "^1.39.8",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.9",
    "github-markdown-css": "^5.8.1",
    "highlight.js": "^11.11.1",
    "jsencrypt": "^3.3.2",
    "katex": "^0.16.22",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.523.0",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "rrule": "^2.8.1",
    "styled-components": "^6.1.19",
    "tippy.js": "^6.3.7",
    "uvcanvas": "^0.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@remix-run/router": "^1.23.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "less-loader": "^12.3.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-bundle-analyzer": "^1.6.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
