{
	"name": "vite-react-antd",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev-mock": "vite --mode mock",
		"build": "tsc -b && pnpm run biome && vite build",
		"build-gzip": "vite build --mode gzip",
		"build-analyze": "vite build --mode analyzer",
		"biome": "biome check --write --no-errors-on-unmatched",
		"preview": "vite preview",
		"update": "pnpm update",
		"reinstall": "npx rimraf node_modules && pnpm install",
		"prepare": "husky"
	},
	"dependencies": {
		"@ant-design/icons": "^5.6.1",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^7.0.0",
		"@dnd-kit/sortable": "^8.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@reduxjs/toolkit": "^2.6.1",
		"antd": "^5.24.6",
		"axios": "^1.8.4",
		"date-fns": "^3.6.0",
		"echarts": "^5.6.0",
		"framer-motion": "^11.18.2",
		"jsencrypt": "^3.3.2",
		"localforage": "^1.10.0",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.428.0",
		"react": "^18.3.1",
		"react-countup": "^6.5.3",
		"react-dom": "^18.3.1",
		"react-infinite-scroll-component": "^6.1.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^6.30.0",
		"styled-components": "^6.1.17",
		"uvcanvas": "^0.3.0",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@remix-run/router": "^1.23.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.17.30",
		"@types/react": "^18.3.20",
		"@types/react-dom": "^18.3.6",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitejs/plugin-react-swc": "^3.8.1",
		"autoprefixer": "^10.4.21",
		"husky": "^9.1.7",
		"less-loader": "^12.2.0",
		"lint-staged": "^15.5.0",
		"postcss": "^8.5.3",
		"rimraf": "^6.0.1",
		"rollup-plugin-bundle-analyzer": "^1.6.6",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3",
		"vite": "^6.2.5",
		"vite-plugin-checker": "^0.8.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-mock": "^3.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,d.ts,tsx,.json}": [
			"biome check --write --no-errors-on-unmatched"
		]
	}
}
