{
	"name": "vite-react-antd",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev-mock": "vite --mode mock",
		"build": "tsc -b && pnpm run biome && vite build",
		"build-gzip": "vite build --mode gzip",
		"build-analyze": "vite build --mode analyzer",
		"biome": "biome check --write --no-errors-on-unmatched",
		"preview": "vite preview",
		"update": "pnpm update",
		"reinstall": "npx rimraf node_modules && pnpm i",
		"prepare": "husky"
	},
	"dependencies": {
		"@ant-design/icons": "^5.5.1",
		"@dnd-kit/core": "^6.1.0",
		"@dnd-kit/modifiers": "^7.0.0",
		"@dnd-kit/sortable": "^8.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@reduxjs/toolkit": "^2.2.7",
		"antd": "^5.20.6",
		"axios": "^1.7.7",
		"date-fns": "^3.6.0",
		"echarts": "^5.5.1",
		"jsencrypt": "^3.3.2",
		"localforage": "^1.10.0",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.428.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-infinite-scroll-component": "^6.1.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.26.2",
		"styled-components": "^6.1.13",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.2",
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@remix-run/router": "^1.19.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.16.5",
		"@types/react": "^18.3.8",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"autoprefixer": "^10.4.20",
		"husky": "^9.1.6",
		"less-loader": "^12.2.0",
		"lint-staged": "^15.2.10",
		"postcss": "^8.4.47",
		"rimraf": "^6.0.1",
		"rollup-plugin-bundle-analyzer": "^1.6.6",
		"tailwindcss": "^3.4.12",
		"typescript": "^5.6.2",
		"vite": "^5.4.6",
		"vite-plugin-checker": "^0.8.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-mock": "^3.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,d.ts,tsx,.json}": [
			"biome check --write --no-errors-on-unmatched"
		]
	}
}
